	// 		pins = "gpio12";
	// 		drive-strength = <2>;
	// 		bias-disable;
	// 		output-high;
	// 	};
	// };

	// nand_pins: nand_pins {
	// 	pullups {
	// 		pins =  "gpio52", "gpio53", "gpio58", "gpio59";
	// 		function = "qpic";
	// 		bias-pull-up;
	// 	};

	// 	pulldowns {
	// 		pins = "gpio54", "gpio55", "gpio56", "gpio57",
	// 			"gpio60", "gpio62", "gpio63", "gpio64",
	// 			"gpio65", "gpio66", "gpio67", "gpio68",
	// 			"gpio69";
	// 		function = "qpic";
	// 		bias-pull-down;
	// 	};
	// };

	sd_pins: sd_pins {
		pinmux {
			function = "sdio";
			pins = "gpio23", "gpio24", "gpio25", "gpio26",
				"gpio28", "gpio29", "gpio30", "gpio31";
			drive-strength = <10>;
		};

		pinmux_sd_clk {
			function = "sdio";
			pins = "gpio27";
			drive-strength = <16>;
		};

		pinmux_sd7 {
			function = "sdio";
			pins = "gpio32";
			drive-strength = <10>;
			bias-disable;
		};
	};
};

&blsp_dma {
	status = "okay";
};

