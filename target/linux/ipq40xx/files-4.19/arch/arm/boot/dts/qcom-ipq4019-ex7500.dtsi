		clock-mode = /bits/8 <2>;

		/* RGB led */
		chan0 {
			chan-name = "ex7500:red:wifi5g";
			led-cur = /bits/ 8 <0x20>;
			max-cur = /bits/ 8 <0x60>;
		};

		chan1 {
			chan-name = "ex7500:green:wifi5g";
			led-cur = /bits/ 8 <0x20>;
			max-cur = /bits/ 8 <0x60>;
		};

		chan2 {
			chan-name = "ex7500:blue:wifi5g";
			led-cur = /bits/ 8 <0x20>;
