		};

		// chan3 {
		// 	chan-name = "ex7500:blue:power32";
		// 	led-cur = /bits/ 8 <0x20>;
		// 	max-cur = /bits/ 8 <0x60>;
		// };
	};
};

&nand {
	pinctrl-0 = <&nand_pins>;
	pinctrl-names = "default";
	status = "okay";

	nand@0 {
		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;


			partition@0 {
				label = "0:SBL1";
				reg = <0x0 0x100000>;
				read-only;
			};

			partition@100000 {
				label = "0:MIBIB";
				reg = <0x100000 0x100000>;
				read-only;
			};

			partition@200000 {
				label = "0:BOOTCONFIG";
				reg = <0x200000 0x100000>;
				read-only;
			};

			partition@300000 {
