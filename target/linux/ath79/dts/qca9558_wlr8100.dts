&pcie0 {
	status = "okay";
};

&uart {
	status = "okay";
};

&pll {
	clocks = <&extosc>;
};

&usb_phy {
	status = "okay";
};

&usb {
	status = "okay";
};

&spi {
	status = "okay";
	num-cs = <1>;

	flash@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "mx25l12805d";
		reg = <0>;
		spi-max-frequency = <25000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "u-boot";
				reg = <0x000000 0x030000>;
				read-only;
			};

			partition@1 {
				label = "u-boot-env";
				reg = <0x030000 0x010000>;
				read-only;
			};

			partition@2 {
				label = "firmware";
				reg = <0x040000 0xf10000>;
			};      

			partition@3 {
				label = "manufacture";
				reg = <0xf50000 0x040000>;
				read-only;
			};

			partition@4 {
				label = "backup";
				reg = <0xf90000 0x010000>;
				read-only;
			};

			partition@5 {
				label = "storage";
				reg = <0xfa0000 0x050000>;
				read-only;
			};
      
			partition@6 {
				label = "art";
				reg = <0xff0000 0x010000>;
				read-only;
			};
		};
	};
};

