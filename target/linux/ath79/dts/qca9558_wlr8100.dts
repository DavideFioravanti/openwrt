/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

#include "qca9557.dtsi"

/ {
	compatible = "sitecom,wlr8100", "qca,qca9557";
	model = "Sitecom WLR-8100";

	memory@0 {
		device_type = "memory";
		reg = <0x0 0x8000000>;
	};

	extosc: ref {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-output-names = "ref";
		clock-frequency = <40000000>;
	};

	leds {
		compatible = "gpio-leds";

		status_amber {
			label = "wlr8100:amber:status";
			gpios = <&gpio 19 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};

		status_red {
			label = "wlr8100:red:status";
			gpios = <&gpio 14 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};

		wps_green {
			label = "wlr8100:green:wps";
			gpios = <&gpio 20 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};

		wps_red {
			label = "wlr8100:red:wps";
			gpios = <&gpio 15 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};

		wifi2g_yellow {
			label = "wlr8100:red:wifi2g";
			gpios = <&gpio 13 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};

		usb_red {
			label = "wlr8100:red:usb";
			gpios = <&gpio 23 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};
	};

	keys {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <20>;

		button@0 {
			label = "reset";
			linux,code = <KEY_RESTART>;
			gpios = <&gpio 21 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};
    
    button@1 {
			label = "qss";
			linux,code = <KEY_WPS_BUTTON>;
			gpios = <&gpio 16 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};
	};
  
  
  // SWITCH CONFIG FROM qcom-ipq8064-wpq864.dts
  	mdio0: mdio {
		#address-cells = <1>;
		#size-cells = <0>;

		compatible = "virtual,mdio-gpio";

		pinctrl-0 = <&mdio0_pins>;
		pinctrl-names = "default";

		gpios = <&qcom_pinmux 1 GPIO_ACTIVE_HIGH &qcom_pinmux 0 GPIO_ACTIVE_HIGH>;

		ethernet-phy@0 {
			device_type = "ethernet-phy";
			reg = <0>;
			qca,ar8327-initvals = <
				0x00004 0x7600000   /* PAD0_MODE */
				0x00008 0x1000000   /* PAD5_MODE */
				0x0000c 0x80        /* PAD6_MODE */
				0x000e4 0x6a545     /* MAC_POWER_SEL */
				0x000e0 0xc74164de  /* SGMII_CTRL */
				0x0007c 0x4e        /* PORT0_STATUS */
				0x00094 0x4e        /* PORT6_STATUS */
				>;
		};

		ethernet-phy@4 {
			device_type = "ethernet-phy";
			reg = <4>;
		};
	};
};

